<UserControl x:Class="Saé.UCGagnePlage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Saé"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Image Source="/images/GagnePlage.png" Stretch="Fill"/>
        <Button x:Name="butSuivant" HorizontalAlignment="Right" VerticalAlignment="Bottom" Height="60" Width="180" Margin="0,0,30,30" Background="{x:Null}" BorderBrush="{x:Null}">
            <Image Source="/images/imgSuivant.png" Stretch="Fill"/>
        </Button>
    </Grid>
</UserControl>
